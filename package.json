      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@stylelint/postcss-css-in-js": "^0.37.2",
